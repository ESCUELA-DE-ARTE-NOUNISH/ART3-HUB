# Private key for deployment (required)
PRIVATE_KEY=your_private_key_here

# Basescan API key for contract verification (required)
BASESCAN_API_KEY=your_basescan_api_key_here

# Factory configuration (optional - defaults provided)
DEPLOYMENT_FEE=0.001  # ETH amount to charge for creating collections
PLATFORM_FEE_PERCENTAGE=250  # Basis points (250 = 2.5%)
FACTORY_OWNER=0x1234567890123456789012345678901234567890  # Who owns the factory

# Example factory owners:
# FACTORY_OWNER=0x742d35Cc6634C0532925a3b8D5C5C6C6C8c8c8c8  # Art3 DAO
# FACTORY_OWNER=0x1234567890123456789012345678901234567890  # Your multisig
# If not set, defaults to deployer address