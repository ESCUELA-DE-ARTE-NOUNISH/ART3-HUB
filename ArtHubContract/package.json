{
  "name": "arthubcontract",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npx hardhat test",
    "test:v4": "npx hardhat test test/Art3HubV4.test.ts",
    "compile": "npx hardhat compile",
    "deploy:baseSepolia": "npx hardhat run scripts/deploy-art3hub-v3.ts --network baseSepolia",
    "deploy:base": "npx hardhat run scripts/deploy-art3hub-v3.ts --network base",
    "deploy:celoSepolia": "npx hardhat run scripts/deploy-art3hub-v3.ts --network celoSepolia",
    "deploy:celo": "npx hardhat run scripts/deploy-art3hub-v3.ts --network celo",
    "deploy:zoraSepolia": "npx hardhat run scripts/deploy-art3hub-v3.ts --network zoraSepolia",
    "deploy:zora": "npx hardhat run scripts/deploy-art3hub-v3.ts --network zora",
    "deploy:v4:baseSepolia": "npx hardhat run scripts/deploy-art3hub-v4.ts --network baseSepolia",
    "deploy:v4:base": "npx hardhat run scripts/deploy-art3hub-v4.ts --network base",
    "deploy:v4:celoSepolia": "npx hardhat run scripts/deploy-art3hub-v4.ts --network celoSepolia",
    "deploy:v4:celo": "npx hardhat run scripts/deploy-art3hub-v4.ts --network celo",
    "deploy:v4:zoraSepolia": "npx hardhat run scripts/deploy-art3hub-v4.ts --network zoraSepolia",
    "deploy:v4:zora": "npx hardhat run scripts/deploy-art3hub-v4.ts --network zora",
    "verify:baseSepolia": "npx hardhat run scripts/verify-contracts.ts --network baseSepolia",
    "verify:celoSepolia": "npx hardhat run scripts/verify-contracts.ts --network celoSepolia",
    "verify:zoraSepolia": "npx hardhat run scripts/verify-contracts.ts --network zoraSepolia",
    "test:nft:base": "npx hardhat run scripts/test-nft-creation.ts --network baseSepolia",
    "test:nft:zora": "npx hardhat run scripts/test-nft-zora.ts --network zoraSepolia",
    "test:nft:celo": "npx hardhat run scripts/test-nft-celo.ts --network celoSepolia",
    "test:v4:base": "npx hardhat run scripts/test-art3hub-v4.ts --network baseSepolia",
    "test:v4:zora": "npx hardhat run scripts/test-art3hub-v4.ts --network zoraSepolia",
    "test:v4:celo": "npx hardhat run scripts/test-art3hub-v4.ts --network celoSepolia",
    "check:balances": "npx hardhat run scripts/check-balances.ts",
    "fund:relayer": "npx hardhat run scripts/fund-relayer.ts",
    "flatten": "npx hardhat flatten contracts/Art3HubSubscriptionV3.sol > flattened/Art3HubSubscriptionV3_flat.sol && npx hardhat flatten contracts/Art3HubFactoryV3.sol > flattened/Art3HubFactoryV3_flat.sol && npx hardhat flatten contracts/Art3HubCollectionV3.sol > flattened/Art3HubCollectionV3_flat.sol",
    "flatten:v4": "npx hardhat flatten contracts/Art3HubSubscriptionV4.sol > flattened/Art3HubSubscriptionV4_flat.sol && npx hardhat flatten contracts/Art3HubFactoryV4.sol > flattened/Art3HubFactoryV4_flat.sol && npx hardhat flatten contracts/Art3HubCollectionV4.sol > flattened/Art3HubCollectionV4_flat.sol"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Art3 Hub NFT Factory and Collection Contracts",
  "dependencies": {
    "@openzeppelin/contracts": "^5.1.0",
    "@openzeppelin/contracts-upgradeable": "^5.1.0",
    "axios": "^1.10.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-toolbox-viem": "^3.0.0",
    "dotenv": "^16.5.0",
    "hardhat": "^2.24.0"
  }
